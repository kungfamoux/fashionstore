{
  "name": "clothing-ecommerce",
  "version": "1.0.0",
  "description": "A full-featured e-commerce website for a clothing brand",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/app.js",
    "build": "npm install",
    "vercel-build": "npm install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": ">=16.x"
  },
  "cacheDirectories": [
    "node_modules",
    "public"
  ],
  "vercel": {
    "rewrites": [
      { "source": "/(.*)", "destination": "/api" }
    ]
  },
  "keywords": [
    "ecommerce",
    "clothing",
    "fashion",
    "nodejs",
    "express",
    "supabase"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.53.0",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^17.2.1",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "http-errors": "^2.0.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
